	keywords = {Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Astrophysics of Galaxies, Astrophysics - Earth and Planetary Astrophysics, planets and satellites: formation, accretion, accretion discs, stars: formation, MHD},
	pages = {3406--3416},
	file = {Forgan et al. - 2017 - On the fragmentation boundary in magnetized self-g.pdf:/Users/zijin/Zotero/storage/CYLXTDMW/Forgan et al. - 2017 - On the fragmentation boundary in magnetized self-g.pdf:application/pdf},
}

