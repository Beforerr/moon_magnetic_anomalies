@article{shaneModelingWaveParticleInteractions2022,
	title = {Modeling {Wave}-{Particle} {Interactions} {With} {Photoelectrons} on the {Dayside} {Crustal} {Fields} of {Mars}},
	volume = {49},
	issn = {1944-8007},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1029/2021GL096941},
	doi = {10.1029/2021GL096941},
	abstract = {Whistler mode waves have been proposed as a crucial mechanism in determining the velocity-space distribution of electrons on the dayside crustal magnetic fields of Mars. A superthermal electron transport model has been unable to reproduce the observed pitch angle distributions on a crustal field line. The two key differences are that the observed pitch angle distributions are much more isotropic and the observed high energy pitch angle distributions have a flux peak at perpendicular pitch angles. We solve the bounce-averaged quasi-linear diffusion equation to calculate the steady-state pitch angle distribution of electrons along a crustal field line when in resonance with whistler mode waves. We perform two simulations, changing the background ionosphere, which affects what energies are in resonance with the whistler mode wave. The wave parameters are chosen based on previous observations of whistlers at Mars. Our results reconcile both qualitative differences between the previous data-model comparisons.},
	language = {en},
	number = {2},
	urldate = {2022-03-17},
	journal = {Geophysical Research Letters},
	author = {Shane, Alexander D. and Liemohn, Michael W.},
	year = {2022},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2021GL096941},
	pages = {e2021GL096941},
	file = {Full Text PDF:/Users/zijin/Zotero/storage/8Q6P4IJR/Shane and Liemohn - 2022 - Modeling Wave-Particle Interactions With Photoelec.pdf:application/pdf},
}

