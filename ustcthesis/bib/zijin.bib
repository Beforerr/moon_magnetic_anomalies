	number = {17},
	urldate = {2022-04-10},
	journal = {Geophysical Research Letters},
	author = {Harada, Yuki and Kasahara, Yoshiya and Nishino, Masaki N. and Kurita, Satoshi and Saito, Yoshifumi and Yokota, Shoichiro and Kumamoto, Atsushi and Takahashi, Futoshi and Shimizu, Hisayoshi},
	year = {2021},
	note = {0 citations (Semantic Scholar/DOI) [2022-04-10]
\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2021GL095260},
	keywords = {Moon, electrostatic waves, Kaguya, electron cyclotron drift instability, electron two stream instability},
	pages = {e2021GL095260},
	file = {Full Text PDF:/Users/zijin/Zotero/storage/4US6U8JT/Harada et al. - 2021 - Global Maps of Solar Wind Electron Modification by.pdf:application/pdf;Snapshot:/Users/zijin/Zotero/storage/5M97QYUR/2021GL095260.html:text/html},
