	month = may,
	year = {2008},
	note = {55 citations (Semantic Scholar/DOI) [2022-01-05]},
	keywords = {Moon, Density cavity, Mini-magnetosphere, Lunar Prospector},
	pages = {941--946},
	file = {Halekas et al. - 2008 - Density cavity observed over a strong lunar crusta.pdf:/Users/zijin/Zotero/storage/UWYLMPPL/Halekas et al. - 2008 - Density cavity observed over a strong lunar crusta.pdf:application/pdf;ScienceDirect Snapshot:/Users/zijin/Zotero/storage/EX2PZXYY/S0032063308000287.html:text/html},
