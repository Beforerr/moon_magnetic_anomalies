	note = {50 citations (Semantic Scholar/DOI) [2022-01-05]
\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2002JA009617},
	keywords = {Moon, MHD, magnetic anomalies, mini-magnetospheres, numerical simulation},
	file = {Harnett and Winglee - 2003 - 2.5-D fluid simulations of the solar wind interact.pdf:/Users/zijin/Zotero/storage/QR8Y3XNC/Harnett and Winglee - 2003 - 2.5-D fluid simulations of the solar wind interact.pdf:application/pdf},
}

@article{halekasDensityCavityObserved2008,
	title = {Density cavity observed over a strong lunar crustal magnetic anomaly in the solar wind: {A} mini-magnetosphere?},
	volume = {56},
	issn = {0032-0633},
	shorttitle = {Density cavity observed over a strong lunar crustal magnetic anomaly in the solar wind},
	url = {https://www.sciencedirect.com/science/article/pii/S0032063308000287},
	doi = {10.1016/j.pss.2008.01.008},
	abstract = {We present observations of what may be the inner region of a lunar mini-magnetosphere. If so, these likely represent the first such observations. Previous studies of solar wind interaction with lunar crustal magnetic fields found increased particle fluxes associated with magnetic amplifications, suggesting a shock/sheath region. The central density cavity expected in the inner mini-magnetosphere (if analogous to other planetary magnetospheres) has proven elusive. We now present Lunar Prospector fly-throughs of a density cavity near a strong crustal magnetic source in the solar wind, and compare these unique observations with typical orbits in the solar wind and wake. We observed the density cavity on two consecutive orbits on July 14, 1999 with optimal viewing geometry, downstream from one of the strongest lunar crustal sources (an anomaly centered at ∼235E, 20S), during very unusual solar wind conditions. We found no other similar features in the solar wind in ∼7 months of low-altitude orbits, suggesting that fully formed lunar mini-magnetospheres are rare and/or difficult to observe from orbit.},
	language = {en},
	number = {7},
	urldate = {2022-01-05},
	journal = {Planetary and Space Science},
