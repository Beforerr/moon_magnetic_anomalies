@incollection{landauCHAPTER11GRAVITATIONAL1975,
	address = {Amsterdam},
	series = {Course of {Theoretical} {Physics}},
	title = {{CHAPTER} 11 - {THE} {GRAVITATIONAL} {FIELD} {EQUATIONS}},
