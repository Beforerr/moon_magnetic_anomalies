	number = {13},
	urldate = {2022-03-07},
	journal = {Geophysical Research Letters},
	author = {Zhang, X.-J. and Mourenas, D. and Artemyev, A. V. and Angelopoulos, V. and Bortnik, J. and Thorne, R. M. and Kurth, W. S. and Kletzing, C. A. and Hospodarsky, G. B.},
	year = {2019},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2019GL083833},
	keywords = {Van Allen Probes, wave packet size, chorus waves, electron acceleration, nonlinear wave particle interaction, THEMIS},
	pages = {7182--7190},
	file = {Snapshot:/Users/zijin/Zotero/storage/58QQ38TP/2019GL083833.html:text/html;Zhang et al. - 2019 - Nonlinear Electron Interaction With Intense Chorus.pdf:/Users/zijin/Zotero/storage/LVDXUQRH/Zhang et al. - 2019 - Nonlinear Electron Interaction With Intense Chorus.pdf:application/pdf},
