@incollection{landauCHAPTERCHARGESELECTROMAGNETIC1975,
	address = {Amsterdam},
	series = {Course of {Theoretical} {Physics}},
	title = {{CHAPTER} 3 - {CHARGES} {IN} {ELECTROMAGNETIC} {FIELDS}},
