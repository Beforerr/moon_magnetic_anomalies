@article{jiaGlobalMHDSimulations2015,
	title = {Global {MHD} simulations of {Mercury}'s magnetosphere with coupled planetary interior: {Induction} effect of the planetary conducting core on the global interaction},
	volume = {120},
	issn = {2169-9402},
	shorttitle = {Global {MHD} simulations of {Mercury}'s magnetosphere with coupled planetary interior},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/2015JA021143},
	doi = {10/ggnnmr},
	abstract = {AbstractMercury's comparatively weak intrinsic magnetic field and its close proximity to the Sun lead to a magnetosphere that undergoes more direct space-weathering interactions than other planets. A unique aspect of Mercury's interaction system arises from the large ratio of the scale of the planet to the scale of the magnetosphere and the presence of a large-size core composed of highly conducting material. Consequently, there is strong feedback between the planetary interior and the magnetosphere, especially under conditions of strong external forcing. Understanding the coupled solar wind-magnetosphere-interior interaction at Mercury requires not only analysis of observations but also a modeling framework that is both comprehensive and inclusive. We have developed a new global MHD model for Mercury in which the planetary interior is modeled as layers of different electrical conductivities that electromagnetically couple to the surrounding plasma environment. This new modeling capability allows us to characterize the dynamical response of Mercury to time-varying external conditions in a self-consistent manner. Comparison of our model results with observations by the MErcury Surface, Space ENvironment, GEochemistry, and Ranging (MESSENGER) spacecraft shows that the model provides a reasonably good representation of the global magnetosphere. To demonstrate the capability to model induction effects, we have performed idealized simulations in which Mercury's magnetosphere is impacted by a solar wind pressure enhancement. Our results show that due to the induction effect, Mercury's core exerts strong global influences on the way Mercury responds to changes in the external environment, including modifying the global magnetospheric structure and affecting the extent to which the solar wind directly impacts the surface. The global MHD model presented here represents a crucial step toward establishing a modeling framework that enables self-consistent characterization of Mercury's tightly coupled planetary interior-magnetosphere system.},
	language = {en},
	number = {6},
	urldate = {2022-01-26},
	journal = {Journal of Geophysical Research: Space Physics},
	author = {Jia, Xianzhe and Slavin, James A. and Gombosi, Tamas I. and Daldorff, Lars K. S. and Toth, Gabor and van der Holst, Bart},
	year = {2015},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/2015JA021143},
	keywords = {Plasma, Solar Wind, Magnetosphere, Core, Induction, Mercury},
	pages = {4763--4775},
	file = {Jia et al. - 2015 - Global MHD simulations of Mercury's magnetosphere .pdf:/Users/zijin/Zotero/storage/CGWLRAGJ/Jia et al. - 2015 - Global MHD simulations of Mercury's magnetosphere .pdf:application/pdf;Snapshot:/Users/zijin/Zotero/storage/9KKPAIHU/2015JA021143.html:text/html},
}

@article{jiaMESSENGERObservationsGlobal2019,
