	year = {2014},
	note = {29 citations (Semantic Scholar/DOI) [2022-05-12]
\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/2014JA019900},
	keywords = {Moon, solar wind interaction, crustal magnetic field, hybrid simulation, particle deflection},
	pages = {6095--6105},
	file = {Full Text PDF:/Users/zijin/Zotero/storage/LCXGEA6X/Fatemi et al. - 2014 - Effects of protons reflected by lunar crustal magn.pdf:application/pdf;Snapshot:/Users/zijin/Zotero/storage/2YC5BUQZ/2014JA019900.html:text/html},
