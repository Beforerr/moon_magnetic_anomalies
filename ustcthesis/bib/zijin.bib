	keywords = {Astrophysics - Earth and Planetary Astrophysics, asteroids: general, minor planets, planets and satellites: formation, comets: general, Kuiper belt: general, meteorites, meteoroids, meteors, Physics - Geophysics, protoplanetary discs},
	pages = {3297--3308},
	file = {Choksi et al. - 2021 - Chondrules from high-velocity collisions thermal .pdf:/Users/zijin/Zotero/storage/J9UXNMHN/Choksi et al. - 2021 - Chondrules from high-velocity collisions thermal .pdf:application/pdf},
