	author = {Artemyev, Anton and Lu, San and El-Alaoui, Mostafa and Lin, Yu and Angelopoulos, Vassilis and Zhang, Xiao-Jia and Runov, Andrei and Vasko, Ivan and Zelenyi, Lev and Russell, Christopher},
	year = {2021},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2020GL092153},
	keywords = {magnetotail, current sheet, substorm},
	pages = {e2020GL092153},
	file = {Snapshot:/Users/zijin/Zotero/storage/RS3GSFZD/2020GL092153.html:text/html},
}

