\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2012GL052818},
	keywords = {Doppler shift, electron energization, ion reflection, lunar crustal magnetic field, upstream, whistler-mode},
	file = {Full Text PDF:/Users/zijin/Zotero/storage/Y2MINGEK/Tsugawa et al. - 2012 - Statistical study of broadband whistler-mode waves.pdf:application/pdf;Snapshot:/Users/zijin/Zotero/storage/5WX5DKGT/2012GL052818.html:text/html},
