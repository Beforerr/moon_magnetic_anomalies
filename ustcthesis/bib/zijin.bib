\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2005RG000184},
	keywords = {Moon, dust, regolith},
	file = {Colwell et al. - 2007 - Lunar surface Dust dynamics and regolith mechanic.pdf:/Users/zijin/Zotero/storage/3FUATU4E/Colwell et al. - 2007 - Lunar surface Dust dynamics and regolith mechanic.pdf:application/pdf;Snapshot:/Users/zijin/Zotero/storage/GMLLWN8K/2005RG000184.html:text/html},
