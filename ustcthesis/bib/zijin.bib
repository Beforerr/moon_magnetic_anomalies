	title = {Atmospheric {Escape} {From} {TOI}-700 d: {Venus} versus {Earth} {Analogs}},
	volume = {896},
	issn = {2041-8205},
	shorttitle = {Atmospheric {Escape} {From} {TOI}-700 d},
