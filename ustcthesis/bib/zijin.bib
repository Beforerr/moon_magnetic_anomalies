	year = {2021},
	note = {2 citations (Semantic Scholar/DOI) [2022-05-12]
\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2020JE006667},
	keywords = {Moon, magnetic fields, Imbrium Basin, impact basins},
	pages = {e2020JE006667},
	file = {Full Text PDF:/Users/zijin/Zotero/storage/5EHC3WI3/Hood et al. - 2021 - A New Large-Scale Map of the Lunar Crustal Magneti.pdf:application/pdf;Snapshot:/Users/zijin/Zotero/storage/AVJ72IK7/2020JE006667.html:text/html},
