	journal = {Reviews of Geophysics},
	author = {Stern, David P.},
	year = {2002},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2000RG000097},
	keywords = {geomagnetic dynamo, geomagnetism, history, lodestone, magnetic survey, solar magnetism},
	pages = {1--1--1--30},
	file = {Full Text PDF:/Users/zijin/Zotero/storage/RZ5DV7GQ/Stern - 2002 - A Millennium of Geomagnetism.pdf:application/pdf;Snapshot:/Users/zijin/Zotero/storage/H9MCYWTP/2000RG000097.html:text/html},
