	file = {Full Text PDF:/Users/zijin/Zotero/storage/XPEENC4R/Nakagawa - 2016 - ULFELF Waves in Near-Moon Space.pdf:application/pdf;Snapshot:/Users/zijin/Zotero/storage/V22RBVND/9781119055006.html:text/html},
