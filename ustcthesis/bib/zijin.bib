	year = {2012},
	note = {10 citations (Semantic Scholar/DOI) [2022-05-16]
\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2012JA017642},
	keywords = {Moon, plasma, electron, nongyrotropic},
	file = {Full Text PDF:/Users/zijin/Zotero/storage/8A8CN466/Harada et al. - 2012 - Nongyrotropic electron velocity distribution funct.pdf:application/pdf;Snapshot:/Users/zijin/Zotero/storage/J4A8DMUF/2012JA017642.html:text/html},
