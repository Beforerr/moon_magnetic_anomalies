@article{zhangMathematicalTreatmentOffset2022,
	title = {A {Mathematical} {Treatment} of the {Offset} {Microlensing} {Degeneracy}},
	url = {http://arxiv.org/abs/2205.05085},
	abstract = {The offset microlensing degeneracy, recently proposed by Zhang et al. (2021), has been shown to generalize the close-wide and inner-outer caustic degeneracies into a unified regime of magnification degeneracy in the interpretation of 2-body planetary microlensing observations. The offset degeneracy states that for two planetary lenses that differ only in the projected star-planet separation (\$s\_\{{\textbackslash}rm A\}{\textbackslash}neq s\_\{{\textbackslash}rm B\}\$), the source trajectory is required to cross their point of equal magnification on the lens-axis, located at \$u=(s\_\{{\textbackslash}rm A\}-1/s\_\{{\textbackslash}rm A\}+s\_\{{\textbackslash}rm B\}-1/s\_\{{\textbackslash}rm B\})/2\$ from the primary, to give rise to degenerate observations. Crucially, the above formalism applies to all three caustic topologies, and implies that the \$s\_\{{\textbackslash}rm A\}=1/s\_\{{\textbackslash}rm B\}\$ solution of the close-wide degeneracy never strictly manifests in microlensing observations unless \$u=0\$. However, the offset degeneracy was proposed upon numerical calculations, and no theoretical justification was given. Here, we provide a theoretical treatment of the offset degeneracy, which demonstrates its nature as a mathematical degeneracy. From first principles, we show that the offset degeneracy formalism is exact to zeroth-order in the mass ratio (\$q\$) for two cases: when the source crosses the lens-axis inside of caustics, and for \$(s\_A-s\_B){\textasciicircum}6{\textbackslash}ll1\$ when crossing outside of caustics. The extent to which the offset degeneracy persists in oblique source trajectories is explored numerically. Additionally, it is shown that the superposition principle allows for a straightforward generalization to \$N\$-body microlenses with \$N\$-1 planetary lens components (\$q{\textbackslash}ll1\$), which results in a \$2{\textasciicircum}\{N-1\}\$-fold degeneracy. Lastly, we demonstrate that the recently proposed "\$s{\textasciicircum}{\textbackslash}dagger\$" heuristic results from the \$s{\textbackslash}rightarrow1\$ limit of the offset degeneracy, and thus does not correctly unify the close-wide and inner-outer degeneracies.},
	urldate = {2022-05-12},
	journal = {arXiv:2205.05085 [astro-ph]},
	author = {Zhang, Keming and Gaudi, B. Scott},
	month = may,
	year = {2022},
	note = {arXiv: 2205.05085},
	keywords = {Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Astrophysics of Galaxies, Astrophysics - Earth and Planetary Astrophysics, Astrophysics - Instrumentation and Methods for Astrophysics, ⛔ No DOI found},
	file = {arXiv Fulltext PDF:/Users/zijin/Zotero/storage/SWHHYDDU/Zhang and Gaudi - 2022 - A Mathematical Treatment of the Offset Microlensin.pdf:application/pdf;arXiv.org Snapshot:/Users/zijin/Zotero/storage/E583EWQY/2205.html:text/html},
