@article{verscharenMultiscaleNatureSolar2019,
	title = {The multi-scale nature of the solar wind},
	volume = {16},
	issn = {1614-4961},
	url = {https://doi.org/10.1007/s41116-019-0021-0},
	doi = {10.1007/s41116-019-0021-0},
	abstract = {The solar wind is a magnetized plasma and as such exhibits collective plasma behavior associated with its characteristic spatial and temporal scales. The characteristic length scales include the size of the heliosphere, the collisional mean free paths of all species, their inertial lengths, their gyration radii, and their Debye lengths. The characteristic timescales include the expansion time, the collision times, and the periods associated with gyration, waves, and oscillations. We review the past and present research into the multi-scale nature of the solar wind based on in-situ spacecraft measurements and plasma theory. We emphasize that couplings of processes across scales are important for the global dynamics and thermodynamics of the solar wind. We describe methods to measure in-situ properties of particles and fields. We then discuss the role of expansion effects, non-equilibrium distribution functions, collisions, waves, turbulence, and kinetic microinstabilities for the multi-scale plasma evolution.},
	language = {en},
	number = {1},
	urldate = {2021-11-24},
	journal = {Living Reviews in Solar Physics},
	author = {Verscharen, Daniel and Klein, Kristopher G. and Maruca, Bennett A.},
	month = dec,
	year = {2019},
	note = {105 citations (Semantic Scholar/DOI) [2022-01-24]},
	pages = {5},
	file = {Verscharen et al. - 2019 - The multi-scale nature of the solar wind.pdf:/Users/zijin/Zotero/storage/I88PGHL5/Verscharen et al. - 2019 - The multi-scale nature of the solar wind.pdf:application/pdf},
}

@article{kleinInferredLinearStability2021,
	title = {Inferred {Linear} {Stability} of {Parker} {Solar} {Probe} {Observations} {Using} {One}- and {Two}-component {Proton} {Distributions}},
	volume = {909},
	issn = {0004-637X},
	url = {https://doi.org/10.3847/1538-4357/abd7a0},
	doi = {10.3847/1538-4357/abd7a0},
	abstract = {The hot and diffuse nature of the Sun’s extended atmosphere allows it to persist in non-equilibrium states for long enough that wave–particle instabilities can arise and modify the evolution of the expanding solar wind. Determining which instabilities arise, and how significant a role they play in governing the dynamics of the solar wind, has been a decades-long process involving in situ observations at a variety of radial distances. With new measurements from the Parker Solar Probe (PSP), we can study what wave modes are driven near the Sun, and calculate what instabilities are predicted for different models of the underlying particle populations. We model two hours-long intervals of PSP/SPAN-i measurements of the proton phase-space density during the PSP’s fourth perihelion with the Sun using two commonly used descriptions for the underlying velocity distribution. The linear stability and growth rates associated with the two models are calculated and compared. We find that both selected intervals are susceptible to resonant instabilities, though the growth rates and kinds of modes driven unstable vary depending on whether the protons are modeled using one or two components. In some cases, the predicted growth rates are large enough to compete with other dynamic processes, such as the nonlinear turbulent transfer of energy, in contrast with relatively slower instabilities at larger radial distances from the Sun.},
	language = {en},
	number = {1},
	urldate = {2021-11-24},
	journal = {The Astrophysical Journal},
	author = {Klein, K. G. and Verniero, J. L. and Alterman, B. and Bale, S. and Case, A. and Kasper, J. C. and Korreck, K. and Larson, D. and Lichko, E. and Livi, R. and McManus, M. and Martinović, M. and Rahmati, A. and Stevens, M. and Whittlesey, P.},
	month = mar,
	year = {2021},
	note = {3 citations (Semantic Scholar/DOI) [2021-11-24]
Publisher: American Astronomical Society},
	pages = {7},
	file = {Klein et al. - 2021 - Inferred Linear Stability of Parker Solar Probe Ob.pdf:/Users/zijin/Zotero/storage/95DQUFTQ/Klein et al. - 2021 - Inferred Linear Stability of Parker Solar Probe Ob.pdf:application/pdf},
}

@article{kleinDiagnosingCollisionlessEnergy2020,
	title = {Diagnosing collisionless energy transfer using field-particle correlations: {Alfvén}-ion cyclotron turbulence},
	volume = {86},
	issn = {0022-3778},
	shorttitle = {Diagnosing collisionless energy transfer using field-particle correlations},
	url = {https://ui.adsabs.harvard.edu/abs/2020JPlPh..86d9002K},
	doi = {10.1017/S0022377820000689},
	abstract = {We apply field-particle correlations - a technique that tracks the time-averaged velocity-space structure of the energy density transfer rate between electromagnetic fields and plasma particles - to data drawn from a hybrid Vlasov-Maxwell simulation of Alfvén-ion cyclotron turbulence. Energy transfer in this system is expected to include both Landau and cyclotron wave-particle resonances, unlike previous systems to which the field-particle correlation technique has been applied. In this simulation, the energy transfer rate mediated by the parallel electric field \$E\_\{{\textbackslash}Vert \}\$ comprises approximately 60 \% of the total rate, with the remainder mediated by the perpendicular electric field \$E\_\{{\textbackslash}bot \}\$. The parallel electric field resonantly couples to protons, with the canonical bipolar velocity-space signature of Landau damping identified at many points throughout the simulation. The energy transfer mediated by \$E\_\{{\textbackslash}bot \}\$ preferentially couples to particles with \$v\_\{tp\}{\textbackslash}lesssim v\_\{{\textbackslash}bot \}{\textbackslash}lesssim 3v\_\{tp\}\$, where \$v\_\{tp\}\$ is the proton thermal speed, in agreement with the expected formation of a cyclotron diffusion plateau. Our results demonstrate clearly that the field-particle correlation technique can distinguish distinct channels of energy transfer using single-point measurements, even at points in which multiple channels act simultaneously, and can be used to determine quantitatively the rates of particle energization in each channel.},
	urldate = {2021-11-24},
	journal = {Journal of Plasma Physics},
	author = {Klein, Kristopher G. and Howes, Gregory G. and TenBarge, Jason M. and Valentini, Francesco},
	month = aug,
	year = {2020},
	note = {9 citations (Semantic Scholar/DOI) [2021-11-24]
ADS Bibcode: 2020JPlPh..86d9002K},
	keywords = {Astrophysics - Solar and Stellar Astrophysics, Physics - Plasma Physics, Physics - Space Physics, plasma simulation, plasma nonlinear phenomena, space plasma physics},
	pages = {905860402},
	file = {Klein et al. - 2020 - Diagnosing collisionless energy transfer using fie.pdf:/Users/zijin/Zotero/storage/NTJBSJ3L/Klein et al. - 2020 - Diagnosing collisionless energy transfer using fie.pdf:application/pdf},
}

