@article{ichimotoHaRedAsymmetry1984,
	title = {Hα red asymmetry of solar flares},
	volume = {93},
	issn = {1573-093X},
	url = {https://doi.org/10.1007/BF00156656},
	doi = {10.1007/BF00156656},
	abstract = {The evolutional characteristics of the red asymmetry of Hα flare line profiles were studied by means of a quantitative analysis of Hα flare spectra obtained with the Domeless Solar Telescope at Hida Observatory. Red-shifted emission streaks of Hα line are found at the initial phase of almost all flares which occur near the disk center, and are considered to be substantial features of the red asymmetry. It is found that a downward motion in the flare chromospheric region is the cause of the red-shifted emission streak. The downward motion abruptly increases at the onset of a flare, attains its maximum velocity of about 40 to 100 km s-1 shortly before the impulsive peak of the microwave burst, and rapidly decreases before the intensity of Hα line reaches its maximum. Referring to the numerical simulations made by Livshits et al. (1981) and Somov et al. (1982), we conclude that the conspicuous red-asymmetry or the red-shifted emission streak of Hα line is due to the downward motion of the compressed chromospheric flare region produced by the impulsive heating by energetic electron beam or thermal conduction.},
	language = {en},
	number = {1},
	urldate = {2022-01-31},
	journal = {Solar Physics},
	author = {Ichimoto, K. and Kurokawa, H.},
	month = jun,
	year = {1984},
	pages = {105--121},
	file = {Springer Full Text PDF:/Users/zijin/Zotero/storage/D6EUMCE2/Ichimoto and Kurokawa - 1984 - Hα red asymmetry of solar flares.pdf:application/pdf},
}

@article{kretzschmarSunStarObservations2011,
	title = {The {Sun} as a star: observations of white-light flares},
	volume = {530},
	copyright = {© ESO, 2011},
	issn = {0004-6361, 1432-0746},
	shorttitle = {The {Sun} as a star},
	url = {https://www.aanda.org/articles/aa/abs/2011/06/aa15930-10/aa15930-10.html},
	doi = {10.1051/0004-6361/201015930},
	abstract = {{\textless}i{\textgreater}Context.{\textless}i/{\textgreater} Solar flares radiate energy at all wavelengths, but the spectral distribution of this energy is still poorly known. White-light continuum emission is sometimes observed, and these flares are then called “white-light flares” (WLFs).{\textless}i{\textgreater}Aims.{\textless}i/{\textgreater} We investigate if all flares are WLFs and how the radiated energy is distributed spectrally.{\textless}i{\textgreater}Methods.{\textless}i/{\textgreater} We perform a superposed epoch analysis of spectral and total irradiance measurements obtained since 1996 by the SOHO and GOES spacecraft at various wavelengths, from soft X-rays to the visible domain.{\textless}i{\textgreater}Results.{\textless}i/{\textgreater} The long-term record of solar irradiance and the excellent duty cycle of the measurements allow us to detect a signal in visible irradiance even for moderate (C-class) flares mainly during the impulsive phase. We identify this signal as continuum emission emitted by WLFs and find that it is consistent with a blackbody emission at {\textasciitilde}9000 K. We estimate the contribution of the WL continuum for several sets of flares and find it to be about 70\% of the total radiated energy. We re-analyse the X17 flare that occurred on 28 October 2003 and find similar results. {\textless}i{\textgreater}Conclusions.{\textless}i/{\textgreater} We show that most of the flares – if not all – are WLFs and that the white-light continuum is the main contributor to the total radiated energy; this continuum is consistent with a blackbody spectrum at {\textasciitilde}9000 K. These observational results are important for understanding the physical mechanisms during flares and possibly suggest a contribution of flares to the variations of the total solar irradiance (TSI).},
	language = {en},
	urldate = {2022-01-31},
	journal = {Astronomy \& Astrophysics},
	author = {Kretzschmar, M.},
	month = jun,
	year = {2011},
	note = {Publisher: EDP Sciences},
	pages = {A84},
	file = {Full Text PDF:/Users/zijin/Zotero/storage/Z5IZW8HC/Kretzschmar - 2011 - The Sun as a star observations of white-light fla.pdf:application/pdf;Snapshot:/Users/zijin/Zotero/storage/93Y4FZJC/aa15930-10.html:text/html},
}

