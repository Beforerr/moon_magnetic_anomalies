	pages = {1834--1853},
	file = {Bennis et al. - 2018 - Ultrareliable and Low-Latency Wireless Communicati.pdf:/Users/zijin/Zotero/storage/2MD555FB/Bennis et al. - 2018 - Ultrareliable and Low-Latency Wireless Communicati.pdf:application/pdf;IEEE Xplore Abstract Record:/Users/zijin/Zotero/storage/YY9Z9B43/8472907.html:text/html},
