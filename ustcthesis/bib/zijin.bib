	pages = {e2021JA029821},
	file = {Full Text PDF:/Users/zijin/Zotero/storage/QCREUIRH/Yeo et al. - 2022 - Laboratory Simulation of Solar Wind Interaction Wi.pdf:application/pdf;Snapshot:/Users/zijin/Zotero/storage/NYUN6HEI/2021JA029821.html:text/html},
