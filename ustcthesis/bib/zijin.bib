	doi = {10.1016/B978-0-12-812700-1.00003-0},
	keywords = {Coronal mass ejections, Extreme solar flares, Solar active regions, Solar energetic particle events, Solar flares, Solar magnetic fields, Space weather},
	pages = {65--98},
	file = {ScienceDirect Full Text PDF:/Users/zijin/Zotero/storage/X57BUAVY/Leka and Barnes - 2018 - Chapter 3 - Solar Flare Forecasting Present Metho.pdf:application/pdf},
}

@article{lundstedtPredictionGeomagneticStorms1994,
	title = {Prediction of geomagnetic storms from solar wind data with the use of a neural network},
	volume = {12},
	issn = {0992-7689},
	url = {https://angeo.copernicus.org/articles/12/19/1994/},
