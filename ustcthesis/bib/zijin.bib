@incollection{tothChallengesModelingOuter2021,
	title = {Challenges in {Modeling} the {Outer} {Magnetosphere}},
	isbn = {978-1-119-81562-4},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781119815624.ch44},
	abstract = {This paper discusses the numerical modeling of the outer magnetosphere. The global magnetosphere is the space surrounding the Earth, including the bow shock, the magnetosphere, and the magnetotail. For sake of limiting the scope, models of the thermosphere, ionosphere, polar wind, radiation belts, and inner magnetosphere are not described here; however, their interaction with the global magnetosphere model is an unavoidable complication that has to be addressed. The major challenges of modeling the outer mangetosphere are the scarcity of observational data, the disparate temporal and spatial scales, and the complexity of the physical system. The outer magnetosphere can be approximated with various physical models ranging from simple single fluid ideal magnetohydrodynamics to full kinetic description of the multispecies space plasma. While the magnetohydrodynamic description is surprisingly successful in getting the global structure approximately right, some of the key processes affecting the dynamics require more sophisticated models. In particular, the magnetic reconnection in the magnetotail plays a crucial role not only in the tail but also for the entire system. We describe several numerical approaches to deal with this diverse set of challenges. The discussion is kept brief: our goal is to describe the main features and point out the important differences among the various physical and numerical approximations, so that nonexpert model users, observers, and data analysts who carry out simulations and/or use simulation results can better understand what these models can and cannot do.},
	language = {en},
