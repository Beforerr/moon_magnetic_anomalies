@article{shaySuperAlfvenicPropagationSubstorm2011,
	title = {Super-{Alfvénic} {Propagation} of {Substorm} {Reconnection} {Signatures} and {Poynting} {Flux}},
	volume = {107},
	issn = {0031-9007, 1079-7114},
	url = {https://link.aps.org/doi/10.1103/PhysRevLett.107.065001},
	doi = {10.1103/PhysRevLett.107.065001},
	abstract = {The propagation of reconnection signatures and their associated energy are examined using kinetic particle-in-cell simulations and Cluster satellite observations. It is found that the quadrupolar out-of-plane magnetic field near the separatrices is associated with a kinetic Alfvén wave. For magnetotail parameters, the parallel propagation of this wave is super-Alfvénic (V∥∼1500–5500 km/s) and generates substantial Poynting flux (S∼10−5–10−4 W/m2) consistent with Cluster observations of magnetic reconnection. This Poynting flux substantially exceeds that due to frozen-in ion bulk outflows and is sufficient to generate white light aurora in Earth’s ionosphere.},
