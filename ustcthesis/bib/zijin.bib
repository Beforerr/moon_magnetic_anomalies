	note = {3 citations (Semantic Scholar/DOI) [2021-11-30]
Publisher: John Wiley \& Sons, Ltd},
	keywords = {solar wind, current sheets, electron instability, Electron Phase space holes, Lunar Wake, solitary structures},
	pages = {4994--5008},
}

