	pages = {567--584},
	file = {Ridley et al. - 2004 - Ionospheric control of the magnetosphere conducta.pdf:/Users/zijin/Zotero/storage/VSDJCEEJ/Ridley et al. - 2004 - Ionospheric control of the magnetosphere conducta.pdf:application/pdf;Snapshot:/Users/zijin/Zotero/storage/KN4KL4HW/2004.html:text/html},
