	keywords = {Lunar wake, cyclotron resonance, whistler mode wave, ULF wave in the solar wind, wake potential structure},
	pages = {569--580},
	file = {Full Text PDF:/Users/zijin/Zotero/storage/EBXKAWHY/Nakagawa et al. - 2003 - GEOTAIL observation of upstream ULF waves associat.pdf:application/pdf;Snapshot:/Users/zijin/Zotero/storage/4HK8KWCP/BF03351789.html:text/html},
