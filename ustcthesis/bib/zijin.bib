@article{plotnikovSynchrotronMaserEmission2019,
	title = {The synchrotron maser emission from relativistic shocks in {Fast} {Radio} {Bursts}: {1D} {PIC} simulations of cold pair plasmas},
	volume = {485},
	issn = {0035-8711},
	shorttitle = {The synchrotron maser emission from relativistic shocks in {Fast} {Radio} {Bursts}},
